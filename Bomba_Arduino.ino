}

void convertSecondsToTime(){
  
    int _seconds = 0;
    int _minutes = 0;
    _minutes     = timeout/60;
    
    Serial.print("minutes: ");
    Serial.println(_minutes);
    
    _seconds     = timeout - (_minutes*60);

    Serial.print("seconds: ");
    Serial.println(_seconds);
    
    String m    = String(_minutes);
    String s    = String(_seconds);

    if (m.length() < 2){
      m = "0" + m;
      Serial.println(m);
    }
    if (s.length() <2){
      s = "0" + s;
      Serial.println(s);
    }
    msg_status = "                ";
    delay(10);
    time_to_show = m+":"+s;
    msg_status = pt_time + time_to_show;
}

void counter(){
  //started se iniciou a contagem...
  if (!started){
    return;
  }
  // ... e finished se conseguiu finalizar o desafio. o started continua porque é só pro time.
  if (!finished){
      timeout -= 1;
      if (timeout < 1){
        digitalWrite(5,HIGH);
        Serial.println("explodiu");
        disable();
      }
      buzzer();
  }
  convertSecondsToTime();
}
